div(class="modal fade bd-example-modal-lg" id="modalhistorialvehiculos" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true")
  div(class="modal-dialog modal-lg" role="document")
    div(class="modal-content")
      div(class="modal-header")
        h5(class="modal-title" id="exampleModalLabel") HISTORIAL DEL VEHICULO
        button(type="button" class="close" data-dismiss="modal" aria-label="Close")
          span(aria-hidden="true") &times;
      div(class="modal-body")
        form(style="border: 8px black inset; font-family: bold" align="center" id="formusuario")          
          .form-row(class="container")
            .form-group.col-md-12
              div(class="alert alert-primary" style="border: 2px blue outset" role="alert")
                | MENSAJE
      div(class="modal-footer")
        <div class="form-group form-check">
          button(type="button" id="btnCerrar" name="btnCerrar" class="btn btn-secondary" data-dismiss="modal" ) CERRAR
          button(button type="button" id="btnGuardarAsignacion" name="btnGuardarAsignacion" class="btn btn-primary") GUARDAR          
        </div>      
        
